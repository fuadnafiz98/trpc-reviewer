{
  "name": "nextjs-starter",
  "description": "A Next.js starter that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "fuadnafiz98 fuadnafiz98<fuadnafiz98@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "export": "next export",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0-alpha.2",
    "@types/node": "16.11.36",
    "@types/prettier": "2.3.2",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.16.0",
    "eslint-config-alloy": "4.5.1",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "globby": "12.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "next-sitemap": "1.6.140",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "tailwind-scrollbar": "1.3.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4"
  }
}
